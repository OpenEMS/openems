{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "chart.js"
            ],
            "baseHref": "/",
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "15mb",
                "maximumError": "20mb"
              }
            ],
            "index": "src/index.html",
            "main": "src/main.ts",
            "namedChunks": false,
            "outputPath": "target",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "tsConfig": "tsconfig.app.json",
            "vendorChunk": false,
            "aot": false,
            "buildOptimizer": false,
            "extractLicenses": false,
            "optimization": false,
            "outputHashing": "none",
            "sourceMap": true
          },
          "configurations": {
            "prod": {
              "aot": true,
              "buildOptimizer": true,
              "extractLicenses": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "serviceWorker": true
            },
            "openems": {
              "assets": [
                "src/assets",
                {
                  "glob": "**/*",
                  "input": "src/themes/openems/root",
                  "output": ""
                },
                {
                  "glob": "**/*.svg",
                  "input": "node_modules/ionicons/dist/ionicons/svg",
                  "output": "./svg"
                }
              ],
              "stylePreprocessorOptions": {
                "includePaths": [
                  "src/themes/openems/scss"
                ]
              },
              "styles": [
                "src/themes/openems/scss/variables.scss",
                "src/global.scss"
              ]
            },
            "openems-backend-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/backend-dev.ts"
                }
              ]
            },
            "openems-backend-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/backend-prod.ts"
                }
              ]
            },
            "openems-edge-dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/edge-dev.ts"
                }
              ]
            },
            "openems-edge-prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/edge-prod.ts"
                }
              ]
            },
            "openems-edge-docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/edge-docker.ts"
                }
              ]
            },
            "openems-backend-docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/backend-docker.ts"
                }
              ]
            },
            "openems-gitpod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/theme.ts",
                  "with": "src/themes/openems/environments/theme.ts"
                },
                {
                  "replace": "src/environments/dummy.ts",
                  "with": "src/themes/openems/environments/gitpod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build:openems,openems-edge-dev"
          },
          "configurations": {
            "openems-backend-dev": {
              "browserTarget": "app:build:openems,openems-backend-dev"
            },
            "openems-edge-dev": {
              "browserTarget": "app:build:openems,openems-edge-dev"
            },
            "openems-backend-prod": {
              "browserTarget": "app:build:openems,openems-backend-prod,prod"
            },
            "openems-edge-prod": {
              "browserTarget": "app:build:openems,openems-edge-prod,prod"
            },
            "openems-gitpod": {
              "browserTarget": "app:build:openems,openems-gitpod"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "scripts": []
          },
          "configurations": {
            "local": {
              "karmaConfig": "karma.conf.local.js"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}